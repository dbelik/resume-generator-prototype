{
    "usesTypeScript": true,
    "usesCssModule": false,
    "cssPreprocessor": "scss",
    "testLibrary": "None",
    "component": {
        "default": {
            "path": "resources/ts/components",
            "withStyle": false,
            "withTest": false,
            "withStory": false,
            "withLazy": false,
            "customTemplates": {
                "component": "resources/ts/templates/default/index.tsx",
                "lazy": "resources/ts/templates/default/Variants.tsx"
            }
        },
        "page": {
            "path": "resources/ts/pages",
            "withLazy": false,
            "withStory": false,
            "withStyle": false,
            "withTest": false,
            "customTemplates": {
                "component": "resources/ts/templates/default/index.tsx"
            }
        }
    }
}
